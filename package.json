{
  "name": "eyc.control.ng-schema-form",
  "version": "1.0.1",
  "description": "Based on https://github.com/makinacorpus/angular2-schema-form",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "install:peers": "install-self-peers",
    "start": "yarn test:updateLib && cd tests && yarn install && yarn start",
    "clean": "rimraf dist/",
    "build:aot": "ngc -p tsconfig.json",
    "build:jit": "tsc -p tsconfig.json",
    "build": "yarn run clean && yarn build:aot",
    "test:updateLib": "cd tests/src && rm -rf ./lib && mkdir lib/ && cp -r ../../src/ lib/eyc.control.ng-schema-form/ && cd -",
    "test": "yarn test:updateLib && cd tests && yarn install && ng test --single-run --code-coverage",
    "testdebug": "yarn test:updateLib && cd tests && ng test --single-run=false --watch",
    "build2": "npm run clean && npm run build:aot && npm version patch && cpx package.json dist",
    "install:deps": "npm i && npm-install-peers"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petrce/angular2-schema-form.git"
  },
  "keywords": [
    "angular",
    "schema",
    "form",
    "schema form"
  ],
  "license": "MIT",
  "dependencies": {
    "@angular-devkit/core": "0.6.1",
    "@angular/compiler": "^6.0.1",
    "body-parser": "1.18.2",
    "methods": "1.1.2",
    "raw-loader": "0.5.1",
    "rxjs": "6.1.0",
    "rxjs-compat": "6.1.0",
    "typescript": "^2.7.2",
    "z-schema": "3.20.0"
  },
  "peerDependencies": {
    "@angular/animations": "6.0.1",
    "@angular/common": "6.0.1",
    "@angular/compiler": "6.0.1",
    "@angular/core": "6.0.1",
    "@angular/forms": "6.0.1",
    "@angular/http": "6.0.1",
    "@angular/platform-browser": "6.0.1",
    "core-js": "2.5.6",
    "rxjs": "6.1.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.1",
    "@angular/platform-server": "6.0.1",
    "@team-griffin/install-self-peers": "1.1.1",
    "@types/jasmine": "2.8.7",
    "@types/node": "10.0.8"
  }
}
